<launch>
  <node pkg="rb1_teleop" type="rb1_teleop_key" name="rb1_teleop_keyboard"  output="screen">
    <param name="scale_linear"  value="0.5" type="double"/>
    <param name="scale_angular" value="0.5" type="double"/>
    <remap from="rb1_teleop_keyboard/cmd_vel" to="pad_teleop/cmd_vel"/>
  </node>
</launch>
